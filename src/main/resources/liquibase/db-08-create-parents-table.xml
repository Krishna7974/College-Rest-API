<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
        http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.8.xsd"
>
    <changeSet id="db-09-create-parents-table" author="Krishna">
        <createTable tableName="parents">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column name="father_name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="mother_name" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="phone_no" type="varchar(255)">
                <constraints nullable="false"/>
            </column>
            <column name="siblings" type="bigint"/>
        </createTable>
    </changeSet>
</databaseChangeLog>
